		"aue": "src/bin/add-user-element.js",
		"list-missing-tests": "src/bin/list-missing-tests.js",
		"lmt": "src/bin/list-missing-tests.js"
	},
	"scripts": {
		"clean": "rm -f src/lib/*.js src/lib/*.map src/bin/*.js src/bin/*.map src/elements/*.js src/elements/*.map test/*.js test/*.map",
		"build:coffee": "coffee -cmb --no-header .",
		"build:peggy": "node ./src/llpeggify.js",
		"build": "clear && npm run build:coffee && npm run build:peggy && node src/bin/low-level-build.js",
		"rebuild": "npm run clean && npm run build",
		"test": "clear && npm run build && npx ava --timeout=1m ./test/*.test.js && git status",
		"tca": "npm run build && node ./src/bin/test-cmd-args.js",
		"fef": "npm run build && node ./src/bin/for-each-file.js"
	},
	"keywords": [
		"coffeescript",
		"javascript",
		"peggy"
	],
	"devDependencies": {
		"ava": "^6.1.3",
		"coffeescript": "^2.7.0",
		"gulp": "^5.0.0",
		"gulp-rename": "^2.0.0",
		"gulp-util": "^3.0.8",
		"npm-run-all": "^4.1.5",
		"through2": "^4.0.2"
